package aufgabe5;

public class Fuse {
	long timeToElapse;

	public Fuse(long timeToElapse) {
		this.timeToElapse = timeToElapse;
	}

	public long getTimeToElapse() {
		return timeToElapse;
	}

	public void setTimeToElapse(long timeToElapse) {
		this.timeToElapse = timeToElapse;
	}
	
	public boolean faster(long otherTimeToElapse) {
		if(timeToElapse < otherTimeToElapse)
			return true;
		else
			return false;
	}
}
