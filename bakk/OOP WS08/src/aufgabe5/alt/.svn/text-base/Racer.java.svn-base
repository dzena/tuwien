package aufgabe5.alt;

/**
 * @author GEMEH
 *
 */
public abstract class Racer implements Comperator<Racer>{
	protected int speed;
	
	public Racer(int speed){
		this.speed = speed;
	}
	public boolean faster(int compSpeed){
		return speed > compSpeed;
	}
	public int speed(){
		return speed;
	}

}