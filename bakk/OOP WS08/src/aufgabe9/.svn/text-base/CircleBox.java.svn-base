package aufgabe9;

public class CircleBox extends Box{
	
	
	public CircleBox(Gift g, float thickness,float height,float radius) {
		super(g,height,thickness);
		this.innerform = new Circle(radius-2*thickness);
		this.outerform = new Circle(radius);
	}
	
	public String getGiftType() {
		return "Box with Circle Base";
	}
}
