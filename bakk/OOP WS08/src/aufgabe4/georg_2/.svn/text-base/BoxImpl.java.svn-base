package aufgabe4.georg_2;

/**
 * @author GEMEH
 *
 */
public abstract class BoxImpl implements Box {
	FillBehavior fillBehavior;
	
	/* (non-Javadoc)
	 * @see aufgabe4.georg_2.Box#print()
	 */
	
	public void print() {
		fillBehavior.print();

	}

	/* (non-Javadoc)
	 * @see aufgabe4.georg_2.Box#setFrameChar(char)
	 */
	
	public void setFrameChar(char fc) {
		fillBehavior.setFrameChar(fc);

	}

	/* (non-Javadoc)
	 * @see aufgabe4.georg_2.Box#setSize(int, int)
	 */
	
	public void setSize(int width, int height) {
		fillBehavior.setSize(width, height);

	}

}
