package swag.gui.cfg;

import javax.faces.bean.ManagedBean;
import javax.faces.bean.RequestScoped;

@ManagedBean
@RequestScoped
public class GuiConfig {
	
	public int getMessagePollingIntervall() {
		return 5; //in seconds
	}
	
	public int getResourcePollingIntervall() {
		return 10; //in seconds
	}
	
	public int getPointPollingIntervall() {
		return 10; //in seconds
	}
	
	public int getBasePollingIntervall() {
		return 10; //in seconds
	}
	
}
